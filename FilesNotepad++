pipeline{
	agent{docker 'maven:3.8.4-alpine'}
		stages{
			stage('Checkout')
			{
			steps
			{
				git'https://github.com/spring-projects/spring-petclinic.git'
				}
				}
				stages('Build')
				{
				steps{
					sh 'mvn clean package'
						junit '**/target/surefire-reports/TEST-*.xml'
						}
						}
						}
						}
						